<ul id="menu" class="sm sm-clean">

  <%= render partial: 'menu/pages', locals: {
    items: Page.top_level_menu_items, path: '/' } %>

  <% plugins_for_main_site_menu.each do |plugin_name| %>
  <%= render partial: "#{plugin_name}/menu/section" %>
  <% end %>

  <% if feature_enabled? :tags %>
  <%= render partial: 'menu/tags' %>
  <% end %>

</ul>
