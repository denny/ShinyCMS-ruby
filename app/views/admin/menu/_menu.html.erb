<ul class="c-sidebar-nav">
  <% plugins_for_admin_menu.each do |plugin| %>
  <%= render partial: "#{plugin.name.underscore}/admin/menu/section" %>
  <% end %>

  <%= render partial: 'admin/menu/menu_users' %>

  <%= render partial: 'admin/menu/menu_other' %>

  <%= render partial: 'admin/menu/menu_stats' %>

  <%= render partial: 'admin/menu/menu_consent_versions' %>

  <%= render partial: 'admin/menu/menu_settings' %>

  <% if sidekiq_web_enabled? %>
  <%= render_admin_menu_item_if( current_user.can?( :manage_sidekiq_jobs ),
      t( 'admin.sidekiq_web.menu' ), sidekiq_web_path, 'applications' ) %>
  <% end %>
</ul>
